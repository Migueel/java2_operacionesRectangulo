public class Rectangulo {

double altura;
double base;

//Metodo para calcular el area de un rectangulo
	public double area; {
	
		double result = (altura * altura) + (base * base);
		return result;
	}

//Metodo para calcular el per√≠metro de un rectangulo
	public double perim; {
		
		double result = altura * base;
		return result;
	}
}
